<?xml version="1.0" encoding="utf-8"?><!-- dialog_quiz_completion.xml -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="vertical"
    android:paddingTop="0dp"
    android:paddingBottom="16dp">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:background="@drawable/edittext_bkg"
        android:fontFamily="@font/noto_sans_math_regular"
        android:gravity="center"
        android:justificationMode="none"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:text="Ουπς! Καλή προσπάθεια, όμως δεν είχες 8 σωστές απαντήσεις. Προσπάθησε ξανά, διαβάζοντας την θεωρία άλλη μία φορά."
        android:textColor="@color/black" />

    <Button
        android:id="@+id/button"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/noto_sans_math_regular"
        android:paddingTop="10dp"
        android:text="Επιστροφή στην θεωρία" />
</LinearLayout>
